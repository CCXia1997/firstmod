package com.ccxia.random;

import java.util.Random;

public class SheepRng {
	
	public static void main(String[] args) //5086938182394799646L
	{
		Random rand=new Random();
		rand.setSeed((long)(459)* 341873128712L+(long)(353)*132897987541L+(12345L) + 10387319L); //-4547468296232501244L
		for (int i = 0; i < 8; ++i) {
	        rand.nextInt();
		}
		int i=rand.nextInt(100);
		System.out.println(i);
		
		for(int x=0;x<5000;x++)
		{
			rand.setSeed((long)(x)* 341873128712L+(long)(353)*132897987541L+(-4547468296232501244L) + 10387319L);
		}
	}

}
