package com.ccxia.random;

import java.util.Random;

public class LightningRng {
	public static void main(String[] args)
	{
		Random rand=new Random();
		int x;
		for(x=0;x<50000;x++)
		{
			rand.setSeed((long)x* 341873128712L+(-4547468296232501244L) + 10387319L);
			for (int i = 0; i < 4; ++i) {
	            rand.nextInt();
	        }
			rand.nextInt(100000);
			rand.nextInt(100000);
			rand.nextInt(100000);
			rand.nextInt(100000);
			int a1=rand.nextInt(100000);
			if(a1==0)
				break;
		}
		System.out.println("¼ÓÔØÇø¿éx×ø±ê£º"+x);
		rand.setSeed((long)459* 341873128712L+(long)353*132897987541L+(12345L) + 10387319L); //10387319
		for (int i = 0; i < 4; ++i) {
            rand.nextInt();
        }
		System.out.println(rand.nextInt(100000));
		System.out.println(rand.nextInt(100000));
		System.out.println(rand.nextInt(100000));
		System.out.println(rand.nextInt(100000));
		System.out.println(rand.nextInt(100000));
		System.out.println(rand.nextInt(100000));
		System.out.println(rand.nextInt(100000));
		System.out.println(rand.nextInt(100000));
		System.out.println(rand.nextInt(100000));
		System.out.println(rand.nextInt(100000));
		System.out.println(rand.nextInt(100000));
		System.out.println(rand.nextInt(100000));
	}

}
