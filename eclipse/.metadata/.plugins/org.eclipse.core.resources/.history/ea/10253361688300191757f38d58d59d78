package com.ccxia.random;

import java.util.Random;

public class BlockDiamondOreRng {
	public static void main(String[] args)
	{
		Random rand=new Random();
		rand.setSeed((long)459* 341873128712L+(long)(353)*132897987541L+(12345L) + 10387319L); //-4547468296232501244L
		for (int i = 0; i < 4; ++i) {
            rand.nextInt();
        }
		int fortuneBonus = rand.nextInt(5) - 1;
		System.out.println(rand.nextInt(5));
		System.out.println(rand.nextInt(5));
		System.out.println(rand.nextInt(5));
		System.out.println(rand.nextInt(5));
		System.out.println(rand.nextInt(5));
		System.out.println(rand.nextInt(5));
	}
	
	

}
