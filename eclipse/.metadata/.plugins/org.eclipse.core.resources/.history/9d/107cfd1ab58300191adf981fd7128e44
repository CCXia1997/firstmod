package com.ccxia.random;

import java.util.Random;

public class BlockDiamondOreRng {
	public static void main(String[] args)
	{
		Random rand=new Random();
		rand.setSeed((long)(1)* 341873128712L+(long)(1)*132897987541L+(123456L) + 10387319L); //-4547468296232501244L
		for (int i = 0; i < 4; ++i) {
            rand.nextInt();
        }
		System.out.println(rand.nextInt(5));
	}
	
	

}
