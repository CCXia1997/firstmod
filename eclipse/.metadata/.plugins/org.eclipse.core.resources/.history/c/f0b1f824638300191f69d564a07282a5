package com.ccxia.random;

import java.util.Random;

public class BlockDiamondOreRng {
	public static void main(String[] args)
	{
		Random rand=new Random();
		rand.setSeed((long)4530* 341873128712L+(long)(0)*132897987541L+(-4547468296232501244L) + 10387319L);
		for (int i = 0; i < 4; ++i) {
            rand.nextInt();
        }

		System.out.println(rand.nextInt(5));
		System.out.println(rand.nextInt(5));
		System.out.println(rand.nextInt(5));
		System.out.println(rand.nextInt(5));
		System.out.println(rand.nextInt(5));
		System.out.println(rand.nextInt(5));
	}
	
	

}
