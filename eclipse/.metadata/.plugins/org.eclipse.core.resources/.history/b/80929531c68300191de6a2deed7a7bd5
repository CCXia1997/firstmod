package com.ccxia.random;

import java.util.Random;

public class IronRng {
	
	
	public static void main(String[] args) //5086938182394799646L
	{
		Random rand=new Random();
		rand.setSeed((long)(15)* 341873128712L+(long)(54)*132897987541L+(5086938182394799646L) + 10387319L); //-4547468296232501244L
		for (int i = 0; i < 8; ++i) {
	        rand.nextInt();
	    }
		for (int i = 0; i < 49; ++i) {
            rand.nextInt();
        }
		rand.nextInt(50);
		int i=rand.nextInt(7000); //i==0
		System.out.println(i);
		System.out.println(rand.nextInt(16)-8);
		System.out.println(rand.nextInt(6)-3);
		System.out.println(rand.nextInt(16)-8);
		
	}
	
}
